    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@types/bluebird": "^3.5.21",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/google-cloud__storage": "^1.1.8",
    "@types/lodash.has": "^4.5.3",
    "@types/lodash.isempty": "^4.4.3",
    "@types/mongoose": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.5.2",
    "@types/raven": "^2.5.1",
    "babel-loader": "8.0.0-beta.2",
    "nodemon": "^1.18.0",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@google-cloud/storage": "^1.7.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "lodash.has": "^4.5.2",
    "lodash.isempty": "^4.4.0",
    "mongoose": "^5.2.2",
