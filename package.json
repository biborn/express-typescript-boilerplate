{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "Express boilerplate with TypeScript.",
  "main": "src/main.ts",
  "repository": "git@github.com:danang-id/express-typescript-boilerplate.git",
  "author": "https://github.com/danang-id",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" DEBUG=\"express-typescript:*\" nodemon",
    "debug": "DEBUG=express-typescript:* nodemon --inspect",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir build --extensions \".ts,.tsx\"",
    "type-check": "tsc --noEmit",
    "bundle": "webpack",
    "production": "node dist/app.bundle.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
